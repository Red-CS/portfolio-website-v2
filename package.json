{
  "name": "portfolio-website-v2",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/react": "^3.0.6",
    "@commitlint/types": "^18.4.3",
    "@sanity/astro": "^2.1.4",
    "@sanity/client": "^6.8.6",
    "@sanity/vision": "^3.20.1",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "astro": "^3.5.7",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "sanity": "^3.20.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^12.5.0",
    "prettier-plugin-astro": "^0.12.2",
    "react-error-overlay": "^6.0.11",
    "vitest": "^1.1.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "none",
    "useTabs": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "overrides": [
      {
        "files": "**/*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  },
  "lint-staged": {
    "*.{js,ts,mjs,astro}": [
      "eslint  --fix",
      "npx prettier --write"
    ],
    "*.{ts,astro}": "npx vitest run"
  }
}
